<h2>Section List</h2>
<hr />
<script src="~/lib/jquery/dist/jquery.min.js"></script>
<script src="https://cdn.jsdelivr.net/npm/ag-grid-community/dist/ag-grid-community.min.js"></script>
@{
    if (Convert.ToString(TempData["msg"]) != "")
    {
        <script> alert("Added Succesfully")</script>
    }
}
<script type="text/javascript">
  
 

    $(document).ready(function() {
      
        $.ajax({
            type: "GET",
            url: "GetSectionList",
           
            contentType: "application/json;charset=utf-8",
            dataType: "json",
            success: function (data) {
      
        const columnDefs = [
                    { headerName: 'Section', field: 'Section' },
                    { headerName: 'Order', field: 'Order' },
                    { headerName: 'Course', field: 'Course' }
                ];
               
                var gridOptions = {
                    columnDefs: columnDefs,
                    rowData: JSON.parse(data)
                };
                const eGridDiv = document.querySelector('#myGrid');

                new agGrid.Grid(eGridDiv, gridOptions);
               
            },
            error: function (response) {
              
               
            }
        });
    });
   
   
</script>
<body>
    <div id="myGrid" style="height: 200px; width:500px;" class="ag-theme-alpine"></div>
</body>

<div>
    <a asp-action="EnrollStudent">Back to Creat</a>
</div>